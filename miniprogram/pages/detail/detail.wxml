<view hover-class="none" hover-stop-propagation="false" style="width: 100%; height: 100%;">
    <map
        id="map" longitude="{{longitude}}" latitude="{{latitude}}" circles="{{circles}}"
        markers="{{markers}}" scale="{{scale}}"  polyline="{{polyline}}"
        show-location show-scale style="width: 100%; height: 100%;"
        bindpoitap="{{bindpoitap}}"
    >
    </map>
</view>